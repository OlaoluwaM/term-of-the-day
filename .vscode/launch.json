  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "program": "${workspaceFolder}/src/index.ts",
      "args": ["--u", "--from", "2021-09-08"],
      "outFiles": ["${workspaceFolder}/build/**/*.js"]
    }
  ]
}
